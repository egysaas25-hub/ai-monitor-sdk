{
  "name": "@aker/ai-monitor-core",
  "version": "1.0.0",
  "description": "Plug-and-play AI monitoring core - drop into any project",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "monitoring",
    "ai",
    "alerts",
    "observability",
    "plug-and-play",
    "notifications"
  ],
  "author": "AKER Team",
  "license": "MIT",
  "peerDependencies": {
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
