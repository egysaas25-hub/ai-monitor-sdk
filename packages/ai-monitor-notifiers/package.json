{
  "name": "@aker/ai-monitor-notifiers",
  "version": "1.0.0",
  "description": "Notification providers for AI Monitor - Telegram, Slack, Email",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "monitoring",
    "notifications",
    "telegram",
    "slack",
    "email",
    "alerts"
  ],
  "author": "AKER Team",
  "license": "MIT",
  "peerDependencies": {
    "telegram": "^2.22.2",
    "axios": "^1.6.5",
    "nodemailer": "^6.9.0"
  },
  "devDependencies": {
    "@aker/ai-monitor-core": "file:../ai-monitor-core",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^6.4.14",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "peerDependenciesMeta": {
    "telegram": {
      "optional": true
    },
    "axios": {
      "optional": true
    },
    "nodemailer": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
