{
  "name": "@aker/ai-monitor-instrumentation",
  "version": "1.0.0",
  "description": "Plug-and-play auto-instrumentation for monitoring any Node.js application",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "keywords": [
    "monitoring",
    "instrumentation",
    "observability",
    "auto-monitor",
    "plug-and-play",
    "performance",
    "errors",
    "metrics"
  ],
  "author": "AKER Team",
  "license": "MIT",
  "peerDependencies": {
    "@aker/ai-monitor-core": "^1.0.0"
  },
  "devDependencies": {
    "@aker/ai-monitor-core": "file:../ai-monitor-core",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
